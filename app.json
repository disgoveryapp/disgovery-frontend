{
    "expo": {
        "name": "Disgovery",
        "slug": "datastone-disgovery",
        "version": "0.1.0",
        "orientation": "portrait",
        "icon": "./assets/disgovery-icon.png",
        "primaryColor": "#7EC13A",
        "splash": {
            "image": "./assets/disgovery-splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#7EC13A"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "userInterfaceStyle": "automatic",
        "ios": {
            "supportsTablet": true,
            "userInterfaceStyle": "automatic",
            "config": {
                "googleMapsApiKey": ""
            },
            "infoPlist": {
                "NSLocationWhenInUseUsageDescription": "Disgovery uses your location to find nearby stops and to navigate you to places. We never share your location with anyone."
            }
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/disgovery-icon.png",
                "backgroundColor": "#7EC13A"
            },
            "userInterfaceStyle": "automatic"
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
